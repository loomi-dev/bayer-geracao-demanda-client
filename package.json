{
  "name": "next-leap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf .next && next dev",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "validate": "yarn type-check && yarn lint && yarn prettier --write .",
    "husky-init-commit": "husky install && husky set .husky/pre-commit \"yarn validate\"",
    "husky-init-push": "husky install && husky set .husky/pre-push \"yarn validate\"",
    "create-icons": "npx @svgr/cli --typescript -d src/components/icons --ignore-existing public/assets/icons",
    "h:module": "hygen module with-prompt && eslint src/modules/**/*.{ts,tsx} --fix",
    "theme": "chakra-cli tokens ./src/styles/theme/index.ts",
    "theme:watch": "chakra-cli tokens ./src/styles/theme/index.ts --watch"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-table": "^8.10.7",
    "axios": "^1.6.2",
    "framer-motion": "^10.12.21",
    "next": "^13.3.0",
    "next-auth": "^4.24.5",
    "nookies": "^2.5.2",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@next/eslint-plugin-next": "^12.1.0",
    "@svgr/webpack": "^8.0.1",
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@types/node": "17.0.21",
    "@types/qs": "^6.9.10",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "8.10.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-sonarjs": "^0.20.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "hygen": "^6.2.11",
    "prettier": "^2.5.1",
    "tsargs": "^1.4.1",
    "typescript": "5.1.6"
  }
}
